<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="251.0" prefWidth="450.0" style="-fx-background-color: linen;" stylesheets="@stylesheet.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.Customer.CheckOutItemController">
   <children>
      <HBox prefHeight="250.0" prefWidth="450.0">
         <children>
            <ImageView fx:id="clothesImg" fitHeight="214.0" fitWidth="184.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@images/clothes/items1.jpg" />
               </image>
               <HBox.margin>
                  <Insets left="15.0" top="15.0" />
               </HBox.margin>
            </ImageView>
            <VBox prefHeight="229.0" prefWidth="265.0">
               <children>
                  <Label fx:id="nameProductLabel" prefHeight="50.0" prefWidth="300.0" text="NAME :">
                     <font>
                        <Font size="17.0" />
                     </font>
                     <padding>
                        <Insets left="15.0" />
                     </padding>
                     <VBox.margin>
                        <Insets top="25.0" />
                     </VBox.margin>
                  </Label>
                  <Label fx:id="priceLabel" prefHeight="50.0" prefWidth="300.0" text="PRICE :">
                     <font>
                        <Font size="17.0" />
                     </font>
                     <padding>
                        <Insets left="15.0" />
                     </padding>
                  </Label>
                  <Label fx:id="sizeLabel" prefHeight="50.0" prefWidth="300.0" text="SIZE :">
                     <font>
                        <Font size="17.0" />
                     </font>
                     <padding>
                        <Insets left="15.0" />
                     </padding>
                  </Label>
                  <Label fx:id="quantityLabel" prefHeight="50.0" prefWidth="300.0" text="QUANTITY :">
                     <font>
                        <Font size="17.0" />
                     </font>
                     <padding>
                        <Insets left="15.0" />
                     </padding>
                  </Label>
               </children>
            </VBox>
         </children>
      </HBox>
   </children>
   <styleClass>
      <String fx:value="shadow" />
      <String fx:value="clothes-item" />
   </styleClass>
</AnchorPane>
